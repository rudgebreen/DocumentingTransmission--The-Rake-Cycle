         <!-- 1. My Jewel, My Joy -->
         
         <lg type="stanza" n="1">
            <l n="1">
               
               <app loc="locE">
                  
                  <rdg wit="#rake1">"My jewel, my joy, don't trouble me with the drum,</rdg>
               </app>
            </l>
            <l n="2">
               <app loc="locE1">
                  <rdg wit="#rake1">Sound the dead march as my corpse goes along,</rdg>
               </app>
            </l>
            <l n="3">
               <app loc="locD2">
                  <rdg wit="#rake1">And over my body throw handfuls of laurel,</rdg>
               </app>
            </l>
            <l n="4">
               <app loc="locE3">
                  <rdg wit="#rake1">And let them all know that I'm going to my rest."</rdg>
               </app>
            </l>
         </lg>
         
         
         <!-- 2. THE UNFORTUNATE RAKE -->
        
         <lg type="stanza" n="1">
         <l n="1">
            <app loc="locA">
               <rdg wit="#rake2">
                  <timeline xml:id="tl_rake1" origin="#tl_rake1_1" unit="s">
                     <when xml:id="tl_rake1_1"/>
                     <when since="#tl_rake1_1" interval="13" xml:id="tl_rake1_2"/>
                  </timeline> As I was a-walking down by
                   </rdg>
            </app>
     
            <app loc="locA">
               <rdg wit="#rake2">
                  <timeline unit="s">
                     <when since="#tl_rake1_2" interval="4" xml:id="tl_rake1_3"/>
                  </timeline>St. James's Hospital<note type="context">Probably London. St. James' Hospital is a recurring motif in the song cycle. It provides a setting for the ballad, giving situational context.</note>,
               </rdg>
            </app></l>
            <l n="2">
            <app loc="locA1">
               <rdg wit="#rake2">
